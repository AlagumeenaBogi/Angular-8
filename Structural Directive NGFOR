import { NgforStrDirComponent } from './ngfor-str-dir/ngfor-str-dir.component';
const routes: Routes = [
    {path: 'ngforcondition', component: NgforStrDirComponent}
];

E:\angularsample\src\app\structural-directive\structural-directive.module.ts
